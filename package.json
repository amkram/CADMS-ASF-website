{
  "name": "nuxtlify",
  "version": "1.0.0",
  "description": "Blog template for Nuxt.js/Netlify",
  "author": "Daniel Kelly",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "before-build": "node -r esm ./build-tools/beforeBuild.js",
    "after-build": "node -r esm ./build-tools/afterBuild.js",
    "dev": "npm run before-build && nuxt -o && npm run after-build",
    "build": "npm run before-build && nuxt build && npm run after-build",
    "start": "nuxt start",
    "generate": "npm run before-build && nuxt generate && npm run after-build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/google-analytics": "^2.3.0",
    "animate.css": "^4.1.0",
    "axios": "^0.21.1",
    "esm": "^3.2.25",
    "gray-matter": "^4.0.2",
    "highlight.js": "^10.1.0",
    "html-loader": "^1.1.0",
    "intersection-observer": "^0.10.0",
    "lodash": "^4.17.15",
    "markdown-it": "^11.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "md5": "^2.2.1",
    "ncp": "^2.0.0",
    "netlify-cms": "^2.10.50",
    "nuxt": "^2.12.2",
    "nuxt-buefy": "^0.3.31",
    "nuxt-fontawesome": "^0.4.0",
    "opti-image": "^0.10.0",
    "react": "^16.13.1",
    "showdown": "^1.9.1",
    "url-parse": "^1.4.7",
    "v-runtime-template": "^1.10.0",
    "vendor": "^1.7.0",
    "vue": "^2.6.11",
    "vue-disqus": "^4.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": ">=14.1.1",
    "eslint-plugin-import": ">=2.21.2",
    "eslint-plugin-jest": ">=23.13.2",
    "eslint-plugin-node": ">=11.1.0",
    "eslint-plugin-nuxt": ">=1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-standard": ">=4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "gray-matter-loader": "0.0.10",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "nuxt-responsive-loader": "^2.0.1",
    "prettier": "^2.0.5",
    "redux": "^4.0.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "sharp": "^0.25.4",
    "slate": "^0.58.3",
    "vue-server-renderer": "^2.6.11"
  }
}
